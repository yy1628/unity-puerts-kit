{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"jsx": "preserve",
		"lib": [
			"ES2020"
		],
		"sourceMap": true,
		"inlineSources": true,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"baseUrl": "src",
		"typeRoots": [
			"Assets/Libraries/Puerts/Typing/puerts",
			"Assets/csharp/gen/Typing/csharp"
		],
		"skipLibCheck": true,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"isolatedModules": true,
		"noEmit": true,
		"forceConsistentCasingInFileNames": true
	},
	"exclude": [
		"src/addons/webapi/**/*.godot.ts",
		"src/addons/webapi/**/*.godot.tsx"
	]
}